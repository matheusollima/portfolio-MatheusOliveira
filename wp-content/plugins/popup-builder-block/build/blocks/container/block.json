      "type": "object",
      "selector": "{{WRAPPER}}",
      "isHover": false
    },
    "showContainerOverlay": {
      "type": "boolean",
      "default": false
    },
    "containerBackgroundOverlay": {
      "type": "object",
      "selector": "{{WRAPPER}}",
      "isHover": true,
      "default": {
        "opacity": {
          "size": "0.5"
        }
      }
    },
    "containerBackgroundHover": {
      "type": "object"
    },
    "containerBackgroundHoverOverlay": {
      "type": "object",
      "default": {
        "opacity": {
          "size": "0.5"
        }
      }
    },
    "containerOverlayHoverTransitionDuration": {
      "type": "object",
      "default": {
        "size": "0.3"
      }
    },
    "commonBorderHoverTransition": {
      "type": "object",
      "default": {
